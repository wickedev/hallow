{
  "name": "@hallow/grpc-web",
  "version": "0.1.0",
  "description": "A gRPC web client library with TypeScript/JavaScript interface and React Hooks support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "typecheck": "tsc --noEmit",
