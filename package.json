    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "rollup": "^3.26.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.0",
    "react": "^18.2.0",
    "tslib": "^2.6.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
